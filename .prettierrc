{
  "arrowParens": "always",
  "bracketSpacing": true,
  "importOrder": ["^#utils", "^#types", "^#e2e", "\\."],
  "importOrderParserPlugins": ["decorators-legacy", "typescript"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 120,
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
