{
  "author": "aleksandr-kiliushin",
  "devDependencies": {
    "@dbml/core": "^2.4.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.1",
    "lint-staged": "^13.0.3",
    "pg": "^8.8.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "18.9.0",
    "npm": "8.19.1"
  },
  "license": "UNLICENSED",
  "name": "db-visualizer",
  "private": false,
  "scripts": {
    "build": "nest build",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "execute": "ts-node ./src/index.ts",
    "init-db": "echo \"bash /var/app/init-db.sh\" | docker exec -i db-visualizer-db bash;",
    "launch-db": "docker-compose -f ./compose.dev.yml up",
    "lint": "eslint \"**/!(migrations)/*.{js,ts}\" --max-warnings=0",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{js,ts,json}\"",
    "remove-db-image": "docker stop db-visualizer-db && docker rm db-visualizer-db && docker rmi db-visualizer_database",
    "test": "jest --runInBand"
  },
  "version": "0.0.1"
}
